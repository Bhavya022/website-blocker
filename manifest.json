{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "activeTab",
      "tabs"
    ],
    "background": {
      "service_worker": "background/background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  